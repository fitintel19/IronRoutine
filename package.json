{
  "name": "iron-routine",
  "version": "1.0.0",
  "description": "Modern fitness generation tool and tracker",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "dev:local": "node server.js",
    "dev:full": "npm run build:assets && npm run dev:local",
    "deploy": "npm run build:workers && wrangler deploy",
    "build": "wrangler deploy --dry-run",
    "build:css": "vite build",
    "build:components": "node scripts/bundle-components.js",
    "build:css:bundle": "node scripts/bundle-css.js",
    "build:embed": "node scripts/embed-for-workers.js",
    "build:assets": "npm run build:css && npm run build:components && npm run build:css:bundle",
    "build:workers": "npm run build:assets && npm run build:embed",
    "dev:css": "vite build --watch",
    "test": "vitest",
    "test:all": "node tests/runner.js",
    "test:api": "node tests/runner.js api",
    "test:paypal": "node tests/runner.js paypal",
    "test:frontend": "node tests/runner.js frontend",
    "test:staging": "node tests/runner.js --environment staging",
    "test:parallel": "node tests/runner.js --parallel",
    "test:parallel:fast": "node tests/runner.js --parallel --concurrency 4",
    "test:verbose": "node tests/runner.js --verbose",
    "test:cleanup": "node tests/runner.js --cleanup",
    "test:data": "node tests/runner.js --test-data",
    "test:json": "node tests/runner.js --format json",
    "test:html": "node tests/runner.js --format html",
    "test:junit": "node tests/runner.js --format junit",
    "test:reports": "node tests/runner.js --format all",
    "lint": "eslint src --ext .js,.ts",
    "format": "prettier --write src",
    "migrate": "node tasks/run-migrations.js",
    "migrate:rollback": "echo 'Run tasks/rollback-migrations.sql in Supabase SQL editor'"
  },
  "keywords": [
    "fitness",
    "workout",
    "tracker",
    "ai",
    "cloudflare"
  ],
  "author": "IronRoutine",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240529.0",
    "@hono/node-server": "^1.17.1",
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "^20.12.12",
    "autoprefixer": "^10.4.21",
    "dotenv": "^17.2.0",
    "eslint": "^9.3.0",
    "postcss": "^8.5.6",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.4.5",
    "vite": "^7.0.5",
    "vitest": "^3.2.4",
    "wrangler": "^4.25.1"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.7.2",
    "@paypal/paypal-server-sdk": "^1.1.0",
    "@supabase/supabase-js": "^2.43.1",
    "canvas-confetti": "^1.9.3",
    "chart.js": "^4.5.0",
    "date-fns": "^4.1.0",
    "hono": "^4.4.2",
    "markdown-it": "^14.1.0",
    "openai": "^4.51.0",
    "prismjs": "^1.30.0",
    "slugify": "^1.6.6",
    "zod": "^4.0.10"
  }
}
