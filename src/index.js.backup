};

const TrackProgress = ({ user }) => {
  const [workoutHistory, setWorkoutHistory] = useState([]);
  const [stats, setStats] = useState({ totalWorkouts: 0, totalMinutes: 0, totalCalories: 0, avgDuration: 0 });
