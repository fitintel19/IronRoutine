    return new Response(await getAppJS(), {
      headers: { 'Content-Type': 'application/javascript' }
    });
  }
  
  if (path === 'style.css') {
    return new Response(await getStyleCSS(), {
      headers: { 'Content-Type': 'text/css' }
    });
  }
  
  return c.notFound();
});

